{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "name": "qbittorrent",
            "title": "qBittorrent",
            "description": "qBittorrent is a cross-platform free and open-source BitTorrent client written in native C++. It relies on Boost, Qt 6 toolkit and the libtorrent-rasterbar library (for the torrent back-end), with an optional search engine written in Python.",
            "categories": [
                "docker"
            ],
            "platform": "linux",
            "logo": "https://upload.wikimedia.org/wikipedia/commons/6/66/New_qBittorrent_Logo.svg",
            "image": "lscr.io/linuxserver/qbittorrent:latest",
            "env": [
                {
                    "name": "PUID",
                    "label": "PUID",
                    "default": "1000"
                },
                {
                    "name": "PGID",
                    "label": "PGID",
                    "default": "1000"
                },
                {
                    "name": "TZ",
                    "label": "TZ",
                    "default": "Asia/Novosibirsk"
                },
                {
                    "name": "WEBUI_PORT",
                    "label": "WEBUI_PORT",
                    "default": "8080"
                }
            ],
            "volumes": [
                {
                    "container": "/config"
                },
                {
                    "container": "/downloads"
                }
            ],
            "ports": [
                "8080:8080/tcp"
            ]
        },
        {
            "type": 1,
            "name": "silverbullet",
            "title": "Silverbullet",
            "description": "SilverBullet is an extensible, [open source](https://github.com/silverbulletmd/silverbullet), **personal knowledge management** system. Indeed, that’s fancy talk for “a note-taking app with links.” However, SilverBullet goes _a bit_ beyond just that.",
            "categories": [
                "docker"
            ],
            "platform": "linux",
            "logo": "https://avatars.githubusercontent.com/u/108344757?s=48&v=4",
            "image": "zefhemel/silverbullet:latest",
            "env": [
                {
                    "name": "SB_HOSTNAME",
                    "label": "SB_HOSTNAME",
                    "default": "0.0.0.0"
                }
            ],
            "volumes": [
                {
                    "container": "/space"
                },
                {
                    "container": "./space"
                }
            ],
            "ports": [
                "3000:3000/tcp"
            ]
        }
    ]
}